  },
  "keywords": [
    "yui",
    "jsdoc",
    "coffeescript",
    "api",
    "documentation",
    "javadoc",
    "docs",
    "apidocs"
  ],
  "main": "./lib/index",
  "bin": {
    "yuidoc": "./lib/cli.js"
  },
  "dependencies": {
    "express": "^4.13.1",
    "graceful-fs": "^4.1.2",
    "markdown-it": "^4.3.0",
    "mdn-links": "^0.1.0",
    "minimatch": "^3.0.2",
    "rimraf": "^2.4.1",
    "yui": "^3.18.1"
  },
  "devDependencies": {
    "eslint": "^8.42.0",
    "istanbul": "^0.3.16",
    "c8": "^7.14.0",
    "mocha": "^10.0.0",
    "selleck": "^0.1.18",
    "ytestrunner": "^0.3.3",
    "yuitest": "^0.7.9"
  },
  "scripts": {
    "linting": "eslint lib/*.js test/*.js",
    "legacy-tests": "istanbul cover --print=both --yui ytestrunner -- --include ./legacy_tests/options.js --include ./legacy_tests/builder.js --include ./legacy_tests/parser.js --include ./legacy_tests/parser_coffee.js --include ./legacy_tests/files.js --include ./legacy_tests/utils.js --include ./legacy_tests/preprocessor.js",
    "test": "c8 mocha"
  },
  "preferGlobal": "true",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "http://github.com/yui/yuidoc.git"
  },
  "yuidoc": {
    "name": "YUIDoc",
    "options": {
      "external": {
        "data": "https://yuilibrary.com/yui/docs/api/data.json"
      },
      "linkNatives": "true",
      "attributesEmit": "true",
      "paths": [
        "./lib"
      ],
      "outdir": "./output/api"
    }
  }
}
